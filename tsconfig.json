{
    "compilerOptions": {
        "module": "es2015",
        "target": "es2015",
        "declaration": true,
        
        "noImplicitAny": true,
        "alwaysStrict": true,
        "strictNullChecks": true,
        "strict": true,
        "noPropertyAccessFromIndexSignature": true,
        "noUnusedLocals": true,
    }
}
