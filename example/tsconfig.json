{
    "compilerOptions": {
        "target": "es2018",
        "module": "esnext",
        "outDir": "./build",
        "sourceMap": true,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "noImplicitAny": true,
        "alwaysStrict": true,
        "lib": ["es2019", "dom"],
        "moduleResolution": "node",
        "jsx": "react",
        "strictNullChecks": true,
        "incremental": true,
        "strict": true,
        "noPropertyAccessFromIndexSignature": true,
        "noUnusedLocals": true
    }
}
